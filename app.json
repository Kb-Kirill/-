{
  "expo": {
    "name": "Predict",
    "slug": "Predict",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "assets": ["./assets/fonts/"],
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "Разрешите нам доступ к Фотопленке, чтобы загрузить фото!",
          "cameraPermission": "Разрешите нам доступ к Камере, чтобы сделать фото!"
        }
      ]
    ],
    "fonts": [
      {
        "asset": "os-bold.ttf"
      }
    ],
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
